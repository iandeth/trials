{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "firebase": "readonly",
    "kzs": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-debugger": ["warn"],
    "no-undef": ["error"],
    "no-unused-vars": ["error", { "varsIgnorePattern": "^(firebase)$" }]
  }
}
